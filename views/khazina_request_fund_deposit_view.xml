<odoo>
<record id="view_form_request_fund_deposit" model="ir.ui.view">
<field name="name"> Request Fund Deposit Form</field>
<field name="model">khazina.request_fund_deposit</field>
<field name="arch" type="xml">
<form class="form">
<header>
<!-- Buttons will go here -->
<field name="state" invisible="True" />
<button name="do_clear_done" type="object"
string="Clear Done"
states="open,cancel"
class="oe_highlight" />


 <field name="state"
widget="statusbar"
options="{'clickable': True}"
statusbar_visible="testimonial,request,order"
/>

</header>
<sheet>
<!-- Content goes here: -->
<group class="groupone">
<field name="recodnumber" />
<field name="recodreference" />
<field name="time"/>
<field name="date" />
<field name="record_editor_name" />
<field name="record_editor_last_name" />
<field name="record_editor_position" />
<div class="container">
  <notebook>
      <page string="طبقا لما هو مدون في المحضرين التاليين">
        <group>
 
 
        </group>
      
      </page>
      <page string="ختم و توقيع">
        <group>
<field name="signature" widget="signature_stu"/>
<field name="secretary_treasury_signature" widget="signature_stu"/>
        </group>
      </page>
    </notebook>
  
</div>
</group>
</sheet>
</form>
</field>
</record>


<record id="view_tree_request_fund_deposit" model="ir.ui.view">
<field name="name">Request Fund Dposit List</field>
<field name="model">khazina.request_fund_deposit</field>
<field name="arch" type="xml">
<tree>
<field name="recodnumber" />
<field name="recodreference" />
<field name="time"/>
<field name="date" />
<field name="state" />
</tree>
</field>
</record>




 


</odoo>